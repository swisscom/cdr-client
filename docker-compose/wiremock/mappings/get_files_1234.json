{
  "mappings":
  [
    {
      "scenarioName": "Get Files for connector 1234 - none there",
      "requiredScenarioState": "Started",
      "newScenarioState": "Started",
      "request": {
        "method": "GET",
        "urlPath": "/documents",
        "queryParameters": {
          "limit": {
            "matches": "1"
          }
        },
        "headers": {
          "cdr-connector-id": {
            "equalTo": "1234"
          },
          "Authorization": {
            "matches": "Bearer .*"
          }
        }
      },
      "response": {
        "status": 204,
        "headers": {
          "Content-Type": "*/*"
        }
      }
    }
  ]
}
